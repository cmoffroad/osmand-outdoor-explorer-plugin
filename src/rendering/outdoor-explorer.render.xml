<?xml version="1.0" encoding="utf-8"?>
<renderingStyle name="Offroad survey" depends="" version="1">

  <!-- PROPERTY - CONTOUR LINES -->
  <renderingProperty attr="contourLines" name="Show contour lines" description="Select minimum zoom level to display in map if available. Separate contour file may be needed." type="string" possibleValues="disabled,16,15,14,13,12,11" defaultValueDescription="default" category="details"/>
  <renderingProperty attr="contourWidth" name="Contour lines width" description="Contour lines width" type="string" possibleValues="thin,medium,thick" defaultValueDescription="default"/>
  <renderingProperty attr="contourDensity" name="Contour lines density" description="Contour lines density" type="string" possibleValues="low,medium_w,high" defaultValueDescription="default"/>

  <!-- <renderingProperty attr="showBoundaries" name="Show Boundaries" description="Show Administrative Boundaries" type="boolean" possibleValues=""
    category="details"/> -->

  <!-- <renderingProperty attr="showFixme" name="Show Fixme" description="Show Fixme Notes" type="boolean" possibleValues=""
    category="details"/> -->

  <renderingProperty attr="activityMode" name="Activity Mode" description="Rendering mode for trail difficulty scales"  type="string" possibleValues="enduro, mtb, hiking" category="details" defaultValue="enduro" />

  <renderingProperty attr="hideUnconfirmed" name="Hide Unconfirmed" description="Tone down unconfirmed tracks and trails" type="boolean" possibleValues="true, false" defaultValue="false"
    category="hide"/>

  <!-- MATERIAL COLORS -->

  <renderingAttribute name="red">
    <case attrColorValue="#f44336" />
  </renderingAttribute>

  <renderingAttribute name="pink">
    <case attrColorValue="#e91e63" />
  </renderingAttribute>

  <renderingAttribute name="purple">
    <case attrColorValue="#9c27b0" />
  </renderingAttribute>

  <renderingAttribute name="deepPurple">
    <case attrColorValue="#673ab7" />
  </renderingAttribute>

  <renderingAttribute name="indigo">
    <case attrColorValue="#3f51b5" />
  </renderingAttribute>

  <renderingAttribute name="blue">
    <case attrColorValue="#2196f3" />
  </renderingAttribute>

  <renderingAttribute name="lightBlue">
    <case attrColorValue="#03a9f4" />
  </renderingAttribute>

  <renderingAttribute name="cyan">
    <case attrColorValue="#00bcd4" />
  </renderingAttribute>

  <renderingAttribute name="yellow">
    <case attrColorValue="#ffeb3b" />
  </renderingAttribute>

  <renderingAttribute name="yellow">
    <case attrColorValue="#ffeb3b" />
  </renderingAttribute>

  <renderingAttribute name="amber">
    <case attrColorValue="#ffc107" />
  </renderingAttribute>

  <renderingAttribute name="orange">
    <case attrColorValue="#ff9800" />
  </renderingAttribute>

  <renderingAttribute name="deepOrange">
    <case attrColorValue="#ff5722" />
  </renderingAttribute>

  <renderingAttribute name="brown">
    <case attrColorValue="#795548" />
  </renderingAttribute>

  <renderingAttribute name="grey">
    <case attrColorValue="#9e9e9e" />
  </renderingAttribute>

  <renderingAttribute name="blueGrey">
    <case attrColorValue="#607d8b" />
  </renderingAttribute>

  <renderingAttribute name="white">
    <case attrColorValue="#FFFFFF" />
  </renderingAttribute>

  <renderingAttribute name="black">
    <case attrColorValue="#000000" />
  </renderingAttribute>

  <renderingAttribute name="transparent">
    <case attrColorValue="#00000000" />
  </renderingAttribute>

  <!-- ATTRIBUTES -->
  <renderingAttribute name="defaultColor">
    <case attrColorValue="#fffbf4" />
  </renderingAttribute>

  <renderingAttribute name="gpsColor">
    <case attrColorValue="#fffb00" />
  </renderingAttribute>

  <renderingAttribute name="missingColor">
    <case hideUnconfirmed="true" attrColorValue="$grey" />
    <case attrColorValue="#fa00ff" />
  </renderingAttribute>

  <renderingAttribute name="buildingColor">
    <case minzoom="14" attrColorValue="$grey" />
  </renderingAttribute>

  <renderingAttribute name="shadowRendering">
    <!-- 0 - no shadow, 1 - one step, 2 - blur shadow, 3 - solid shadow -->
    <case minzoom="13" attrIntValue="3"/>
    <case minzoom="6" maxzoom="12" attrIntValue="3" />
    <case attrIntValue="0"/>
  </renderingAttribute>

  <!-- PATHS -->

  <renderingAttribute name="pathStrokeColor">
    <apply minzoom="13">
      <switch>
        <case activityMode="enduro" tag="highway" value="steps" />
        <case activityMode="enduro" additional="dirtbike:scale=6" />
        <case activityMode="enduro" additional="smoothness=impassable" />
        <case activityMode="enduro" additional="motorcycle=no" />
        <case activityMode="mtb" tag="highway" value="steps" />
        <case activityMode="mtb" additional="smoothness=impassable" />
        <case activityMode="mtb" additional="mtb:scale=6" />
        <case activityMode="mtb" additional="bicycle=no" />
        <apply attrColorValue="$red" pathEffect="3_3" />
      </switch>
      <apply>
        <case additional="survey=ground" />
        <case attrColorValue="$missingColor" />
      </apply>
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <case additional="surface=paving_stones" />
        <apply attrColorValue="$white" />
      </switch>
      <switch>
        <case activityMode="enduro" additional="motorcycle=yes" />
        <case activityMode="enduro" additional="dirtbike:scale=" />
        <case activityMode="mtb" additional="bicycle=yes" />
        <case activityMode="mtb" additional="mtb:scale=" />
        <case activityMode="hiking" />
        <apply attrColorValue="$black" pathEffect="">
          <apply_if activityMode="enduro" additional="dirtbike:scale=0" pathEffect="3_3" />
        </apply>
      </switch>
      <case attrColorValue="$black" pathEffect="3_3" />
    </apply>
  </renderingAttribute>

  <renderingAttribute name="pathCasingColor">
    <apply minzoom="13">
      <switch>
        <case activityMode="enduro" tag="highway" value="steps" />
        <case activityMode="enduro" additional="dirtbike:scale=6" />
        <case activityMode="enduro" additional="smoothness=impassable" />
        <case activityMode="enduro" additional="motorcycle=no" />
        <case activityMode="mtb" tag="highway" value="steps" />
        <case activityMode="mtb" additional="smoothness=impassable" />
        <case activityMode="mtb" additional="mtb:scale=6" />
        <case activityMode="mtb" additional="bicycle=no" />
        <apply attrColorValue="$transparent" pathEffect="3_3" />
      </switch>
      <apply>
        <case additional="survey=ground" />
        <case attrColorValue="$transparent" />
      </apply>
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <case additional="surface=paving_stones" />
        <apply attrColorValue="$black" />
      </switch>
      <apply activityMode="enduro">
        <case additional="dirtbike:scale=5" attrColorValue="$pink" />
        <case additional="dirtbike:scale=4" attrColorValue="$orange" />
        <case additional="dirtbike:scale=3" attrColorValue="$yellow" />
        <case additional="dirtbike:scale=2" attrColorValue="$transparent" />
        <case additional="dirtbike:scale=1" attrColorValue="$transparent" />
        <case additional="dirtbike:scale=0" attrColorValue="#fa00ff" />
      </apply>
      <apply activityMode="hiking">
        <case additional="sac_scale=difficult_alpine_hiking" attrColorValue="$pink" />
        <case additional="sac_scale=demanding_alpine_hiking" attrColorValue="$pink" />
        <case additional="sac_scale=alpine_hiking" attrColorValue="$pink" />
        <case additional="sac_scale=demanding_mountain_hiking" attrColorValue="$orange" />
        <case additional="sac_scale=mountain_hiking" attrColorValue="$yellow" />
        <case additional="sac_scale=hiking" attrColorValue="$transparent" />
      </apply>
      <apply activityMode="mtb">
        <case additional="mtb:scale=6" attrColorValue="$pink" />
        <case additional="mtb:scale=5" attrColorValue="$pink" />
        <case additional="mtb:scale=4" attrColorValue="$pink" />
        <case additional="mtb:scale=3" attrColorValue="$orange" />
        <case additional="mtb:scale=2" attrColorValue="$yellow" />
        <case additional="mtb:scale=1" attrColorValue="$transparent" />
        <case additional="mtb:scale=0" attrColorValue="$transparent" />
      </apply>
      <case attrColorValue="$transparent" />
    </apply>
  </renderingAttribute>

  <!-- MINOR ROADS -->

  <renderingAttribute name="minorRoadStrokeColor">
    <case tag="highway" value="tertiary" maxzoom="12" attrColorValue="$white" />
    <case tag="highway" value="track" maxzoom="12" attrColorValue="$grey" />
    <case tag="highway" value="unclassified" maxzoom="12" attrColorValue="$grey" />
    <apply minzoom="13">
      <apply>
        <case additional="survey=ground" />
        <case tag="highway" value="track" attrColorValue="$missingColor" />
        <case additional="surface=unpaved" attrColorValue="$missingColor" />
        <case attrColorValue="$grey" />
      </apply>
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <case additional="surface=paving_stones" />
        <apply attrColorValue="$white" />
      </switch>
      <case additional="surface=" attrColorValue="$black" />
      <switch>
        <case tag="highway" value="path" />
        <case tag="highway" value="track" />
        <apply attrColorValue="$black" />
      </switch>
      <case attrColorValue="$white" />
    </apply>
  </renderingAttribute>

  <renderingAttribute name="minorRoadCasingColor">
    <case tag="highway" value="tertiary" maxzoom="12" attrColorValue="$black" />
    <apply minzoom="13">
      <apply>
        <case additional="survey=ground" />
        <case attrColorValue="$transparent" />
      </apply>
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <case additional="surface=paving_stones" />
        <apply attrColorValue="$black" />
      </switch>
      <switch>
        <case additional="surface=" />
        <case tag="highway" value="path" />
        <case tag="highway" value="track" />
        <apply>
          <apply activityMode="enduro">
            <case additional="dirtbike:scale=5" attrColorValue="$pink" />
            <case additional="dirtbike:scale=4" attrColorValue="$orange" />
            <case additional="dirtbike:scale=3" attrColorValue="$yellow" />
            <case additional="dirtbike:scale=2" attrColorValue="$transparent" />
            <case additional="dirtbike:scale=1" attrColorValue="$transparent" />
          </apply>
          <apply activityMode="hiking">
            <case additional="sac_scale=difficult_alpine_hiking" attrColorValue="$pink" />
            <case additional="sac_scale=demanding_alpine_hiking" attrColorValue="$pink" />
            <case additional="sac_scale=alpine_hiking" attrColorValue="$pink" />
            <case additional="sac_scale=demanding_mountain_hiking" attrColorValue="$orange" />
            <case additional="sac_scale=mountain_hiking" attrColorValue="$yellow" />
            <case additional="sac_scale=hiking" attrColorValue="$transparent" />
          </apply>
          <apply activityMode="mtb">
            <case additional="mtb:scale=6" attrColorValue="$pink" />
            <case additional="mtb:scale=5" attrColorValue="$pink" />
            <case additional="mtb:scale=4" attrColorValue="$pink" />
            <case additional="mtb:scale=3" attrColorValue="$orange" />
            <case additional="mtb:scale=2" attrColorValue="$yellow" />
            <case additional="mtb:scale=1" attrColorValue="$transparent" />
            <case additional="mtb:scale=0" attrColorValue="$transparent" />
          </apply>
          <case attrColorValue="$transparent" />
        </apply>
      </switch>
      <case attrColorValue="$black" />
    </apply>
  </renderingAttribute>

  <!-- MAJOR ROADS -->

  <renderingAttribute name="majorRoadStrokeColor">
    <case maxzoom="12" attrColorValue="$white" />
    <apply minzoom="13">
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <case additional="surface=paving_stones" />
        <apply attrColorValue="$white" />
      </switch>
      <case additional="surface=" attrColorValue="$black" />
      <case attrColorValue="$white" />
    </apply>
  </renderingAttribute>

  <renderingAttribute name="majorRoadCasingColor">
    <case maxzoom="12" attrColorValue="$black" />
    <apply minzoom="13">
      <switch>
        <case additional="surface=paved" />
        <case additional="surface=concrete" />
        <case additional="surface=concrete:plates" />
        <case additional="surface=concrete:lanes" />
        <case additional="surface=asphalt" />
        <case additional="surface=chipseal" />
        <apply attrColorValue="$black" />
      </switch>
      <case additional="surface=" attrColorValue="$transparent" />
      <case attrColorValue="$black" />
    </apply>
  </renderingAttribute>

  <renderingAttribute name="gpx">
    <switch>
      <case color="$gpsColor" />
      <apply cap="ROUND">
        <case>
          <apply_if minzoom="3" maxzoom="5" strokeWidth="1"/>
          <apply_if minzoom="6" maxzoom="11" strokeWidth="3"/>
          <apply_if minzoom="12" strokeWidth="5"/>
        </case>
      </apply>
    </switch>
  </renderingAttribute>

  <renderingAttribute name="waterColor">
    <case attrColorValue="#AA7fc0ee"/>
  </renderingAttribute>

  <renderingAttribute name="woodColor">
    <case attrColorValue="#c4ecb4"/>

<!--     <case attrColorValue="#c8e9c3"/> -->
  </renderingAttribute>

  <renderingAttribute name="contourLineColor">
    <case attrColorValue="#AAAAAACC">
    </case>
  </renderingAttribute>
  <renderingAttribute name="contourLineColor50m">
    <case attrColorValue="#AAAAAACC">
    </case>
  </renderingAttribute>
  <renderingAttribute name="contourLineColor100m">
    <case attrColorValue="#AAAAAACC">
    </case>
  </renderingAttribute>

  <renderingAttribute name="landuseResidentialColor">
    <case attrColorValue="#CCCCCC"/>
  </renderingAttribute>

  <renderingAttribute name="landuseOrchardColor">
    <case attrColorValue="#bbeca1"/>
  </renderingAttribute>

  <renderingAttribute name="grassColor">
    <case attrColorValue="#E5FFE8"/>
  </renderingAttribute>
  <renderingAttribute name="farmlandColor">
    <case attrColorValue="#FFF6E3"/>
  </renderingAttribute>
  <renderingAttribute name="farmyardColor">
    <case attrColorValue="#EEE5D9"/>
  </renderingAttribute>
  <renderingAttribute name="desertColor">
    <case attrColorValue="#fff7bf"/>
  </renderingAttribute>
  <renderingAttribute name="sandColor">
    <case attrColorValue="#ffe1ba"/>
  </renderingAttribute>
  <renderingAttribute name="beachColor">
    <case attrColorValue="#FFD9A7"/>
  </renderingAttribute>

  <renderingConstant name="highwayTrackTextHaloRadiusDay" value="1"/>

  <renderingAttribute name="highwayTrackTextHaloColor">
    <case nightMode="true" attrColorValue="#99333333"/>
    <case attrColorValue="#ffffff"/>
  </renderingAttribute>

  <renderingConstant name="highwayTrackTextHaloColorDay" value="#aaf1eae4"/>

  <renderingConstant name="boundaryNationalParkColor" value="#a0800080"/>
  <renderingConstant name="boundaryNationalParkColor2" value="#5009dd09"/>

  <renderingAttribute name="waterTextHaloColor">
    <case attrColorValue="#FFFFFF"/>
  </renderingAttribute>

  <renderingConstant name="" value=""/>

  <renderingAttribute name="boundaryColorInner">
    <case minzoom="12" attrColorValue="#aa0002B4"/>
    <case attrColorValue="#990002B4"/>
  </renderingAttribute>
  <renderingAttribute name="boundaryColorOuter">
    <case minzoom="12" attrColorValue="#500002B4"/>
    <case attrColorValue="#300002B4"/>
  </renderingAttribute>

  <renderingAttribute name="placeCityVillageTextColor">
    <case attrColorValue="#000000"/>
  </renderingAttribute>
  <renderingConstant name="placeTextHaloRadiusDay" value="1:1"/>
  <renderingAttribute name="placeCityVillageTextHaloColor">
    <case attrColorValue="#ffffff"/>
  </renderingAttribute>

  <order>
    <switch>
      <!-- ORDER - POINT -->
      <switch area="false" point="true" objectType="1" order="128">
        <case tag="" value=""/>
        <case tag="natural" value=""/>
        <case tag="osmand" value="file_waypoint"/>
        <case tag="osmand" value="fav"/>
        <case tag="barrier" value="" />
        <case tag="amenity" value="fuel"/>
        <case tag="noexit" value="" />
        <case tag="ford" value="yes"/>
        <case tag="waterway" value="waterfall"/>
        <case tag="fixme_tag" value="yes"/>
        <case tag="note_tag" value="yes"/>
        <case tag="description_tag" value="yes"/>
        <case tag="natural" value="peak">
          <apply_if additional="ele_limit=200" maxzoom="12" order="-1"/>
          <apply_if additional="ele_limit=500" maxzoom="11" order="-1"/>
        </case>
      </switch>

      <!-- ORDER - POLYGON -->
      <switch area="true" point="false" addPoint="true" objectType="3" order="5">
        <case tag="place" value="island" order="1"/>
        <case tag="place" value="islet" order="1"/>
        <switch layer="-1" order="3">
          <case tag="natural" value="coastline"/>
          <case tag="natural" value="land"/>
          <case tag="place" value="island"/>
          <case tag="place" value="islet"/>
          <case tag="" value=""/>
        </switch>
      </switch>

      <!-- ORDER - LANDUSE -->
      <switch cycle="true" point="false" addPoint="true" objectType="3" order="5">
        <case tag="natural" value="water" order="9"/>
        <case tag="waterway" value="dock"/>
        <case tag="landuse" value="reservoir" order="9"/>
        <case tag="waterway" value="riverbank" order="9"/>
        <case tag="natural" value="wetland" order="8"/>
        <case tag="natural" value="scrub" order="7"/>
        <case tag="natural" value="wood" order="7"/>
        <case tag="landuse" value="forest" order="7"/>
        <case tag="wood:lost" value="" order="7"/>
        <case tag="amenity" value="place_of_worship" ignorePolygonAsPointArea="true"/>
        <!-- <case tag="fixme_tag" value="yes" order="11"/> -->
        <case tag="" value="" order="5"/>
      </switch>

      <!-- ORDER - LINE -->
      <switch area="false" objectType="2" order="80">
        <case tag="fixme_tag" value="yes" order="12"/>
        <case tag="description_tag" value="yes" order="13"/>
        <case tag="note_tag" value="yes" order="14"/>
        <case tag="name" value="" order="11"/>
        <!-- default line -->
        <case layer="-1" tag="" value="" order="11"/>
        <case tag="" value="" order="12"/>
        <case tag="natural" value="coastline_broken" order="63"/>
        <case tag="natural" value="coastline_line" order="63"/>
        <case tag="osmand" value="file_trackline"/>
        <case tag="osmand" value="file_routeline"/>
        <case tag="admin_level" value="" order="62"/>
        <switch>
          <case tag="highway" value="motorway" order="84">
            <apply_if layer="1" order="107"/>
            <apply_if additional="layer=2" order="137"/>
            <apply_if additional="layer=3" order="165"/>
            <apply_if layer="-1" order="62"/>
            <apply_if additional="layer=-2" order="41"/>
          </case>
          <case tag="highway" value="motorway_link" order="71">
            <apply_if layer="1" order="94"/>
            <apply_if additional="layer=2" order="124"/>
            <apply_if additional="layer=3" order="152"/>
            <apply_if layer="-1" order="49"/>
            <apply_if additional="layer=-2" order="25"/>
          </case>
          <case tag="highway" value="trunk" order="83">
            <apply_if layer="1" order="106"/>
            <apply_if additional="layer=2" order="136"/>
            <apply_if additional="layer=3" order="164"/>
            <apply_if layer="-1" order="61"/>
            <apply_if additional="layer=-2" order="40"/>
          </case>
          <case tag="highway" value="trunk_link" order="70">
            <apply_if layer="1" order="93"/>
            <apply_if additional="layer=2" order="123"/>
            <apply_if additional="layer=3" order="151"/>
            <apply_if layer="-1" order="48"/>
            <apply_if additional="layer=-2" order="24"/>
          </case>
          <case tag="highway" value="primary" order="82">
            <apply_if layer="1" order="105"/>
            <apply_if additional="layer=2" order="135"/>
            <apply_if additional="layer=3" order="163"/>
            <apply_if layer="-1" order="60"/>
            <apply_if additional="layer=-2" order="39"/>
          </case>
          <case tag="highway" value="primary_link" order="69">
            <apply_if layer="1" order="92"/>
            <apply_if additional="layer=2" order="122"/>
            <apply_if additional="layer=3" order="150"/>
            <apply_if layer="-1" order="47"/>
            <apply_if additional="layer=-2" order="23"/>
          </case>
          <case tag="highway" value="secondary" order="81">
            <apply_if layer="1" order="104"/>
            <apply_if additional="layer=2" order="134"/>
            <apply_if additional="layer=3" order="162"/>
            <apply_if layer="-1" order="59"/>
            <apply_if additional="layer=-2" order="38"/>
          </case>
          <case tag="highway" value="secondary_link" order="68">
            <apply_if layer="1" order="91"/>
            <apply_if additional="layer=2" order="121"/>
            <apply_if additional="layer=3" order="149"/>
            <apply_if layer="-1" order="46"/>
            <apply_if additional="layer=-2" order="22"/>
          </case>
          <case tag="highway" value="tertiary" order="80">
            <apply_if layer="1" order="103"/>
            <apply_if additional="layer=2" order="133"/>
            <apply_if additional="layer=3" order="161"/>
            <apply_if layer="-1" order="58"/>
            <apply_if additional="layer=-2" order="37"/>
          </case>
          <case tag="highway" value="tertiary_link" order="67">
            <apply_if layer="1" order="90"/>
            <apply_if additional="layer=2" order="120"/>
            <apply_if additional="layer=3" order="148"/>
            <apply_if layer="-1" order="45"/>
            <apply_if additional="layer=-2" order="21"/>
          </case>
          <case tag="highway" value="road" order="79">
            <apply_if layer="1" order="102"/>
            <apply_if additional="layer=2" order="132"/>
            <apply_if additional="layer=3" order="160"/>
            <apply_if layer="-1" order="57"/>
            <apply_if additional="layer=-2" order="36"/>
          </case>
          <case tag="highway" value="unclassified" order="79">
            <apply_if layer="1" order="102"/>
            <apply_if additional="layer=2" order="132"/>
            <apply_if additional="layer=3" order="160"/>
            <apply_if layer="-1" order="57"/>
            <apply_if additional="layer=-2" order="36"/>
          </case>
          <case tag="highway" value="residential" order="78">
            <apply_if layer="1" order="101"/>
            <apply_if additional="layer=2" order="131"/>
            <apply_if additional="layer=3" order="159"/>
            <apply_if layer="-1" order="56"/>
            <apply_if additional="layer=-2" order="35"/>
          </case>
          <case tag="highway" value="service" order="78">
            <apply_if layer="1" order="101"/>
            <apply_if additional="layer=2" order="131"/>
            <apply_if additional="layer=3" order="159"/>
            <apply_if layer="-1" order="56"/>
            <apply_if additional="layer=-2" order="35"/>
          </case>
          <case tag="highway" value="busway" order="77">
            <apply_if layer="1" order="100"/>
            <apply_if additional="layer=2" order="130"/>
            <apply_if additional="layer=3" order="158"/>
            <apply_if layer="-1" order="55"/>
            <apply_if additional="layer=-2" order="34"/>
          </case>
          <case tag="highway" value="living_street" order="77">
            <apply_if layer="1" order="100"/>
            <apply_if additional="layer=2" order="130"/>
            <apply_if additional="layer=3" order="158"/>
            <apply_if layer="-1" order="55"/>
            <apply_if additional="layer=-2" order="34"/>
          </case>
          <case tag="highway" value="pedestrian" order="76">
            <apply_if activityMode="pedestrian" order="82"/>
            <apply_if layer="1" order="99"/>
            <apply_if additional="layer=2" order="129"/>
            <apply_if additional="layer=3" order="157"/>
            <apply_if layer="-1" order="54"/>
            <apply_if additional="layer=-2" order="33"/>
          </case>
          <case tag="highway" value="track" order="64">
            <apply_if layer="1" order="89"/>
            <apply_if additional="layer=2" order="119"/>
            <apply_if additional="layer=3" order="147"/>
            <apply_if layer="-1" order="20"/>
            <apply_if additional="layer=-2" order="19"/>
          </case>
          <case tag="highway" value="footway" order="64">
            <apply_if layer="1" order="110"/>
            <apply_if additional="layer=2" order="140"/>
            <apply_if additional="layer=3" order="166"/>
            <apply_if layer="-1" order="20"/>
            <apply_if additional="layer=-2" order="19"/>
          </case>
          <case tag="highway" value="steps" order="65">
            <apply_if layer="1" order="111"/>
            <apply_if additional="layer=2" order="140"/>
            <apply_if additional="layer=3" order="166"/>
            <apply_if layer="-1" order="20"/>
            <apply_if additional="layer=-2" order="19"/>
          </case>
          <case tag="highway" value="path" order="37">
            <apply_if layer="1" order="110"/>
            <apply_if additional="layer=2" order="140"/>
            <apply_if additional="layer=3" order="166"/>
            <apply_if layer="-1" order="20"/>
            <apply_if additional="layer=-2" order="19"/>
          </case>
        </switch>
        <case tag="highway" value="bridleway" order="53"/>
        <case tag="barrier" value="" order="20" />
        <case tag="ford" value="yes" order="85"/>
        <case tag="waterway" value="waterfall" order="180"/>
        <case tag="waterway" value="">
          <apply_if layer="1" order="89"/>
          <apply_if additional="layer=2" order="120"/>
          <apply_if additional="layer=3" order="148"/>
        </case>
        <case tag="contour" value="elevation" order="11">
          <apply_if contourDensity="medium_w" additional="contourtype=5m" order="-1"/>
          <apply_if contourDensity="medium_w" additional="contourtype=10m" order="-1"/>
          <apply_if contourDensity="medium_w" additional="contourtype=50m" order="-1"/>
          <apply_if contourDensity="low" additional="contourtype=5m" order="-1"/>
          <apply_if contourDensity="low" additional="contourtype=10m" order="-1"/>
          <apply_if contourDensity="low" additional="contourtype=20m" order="-1"/>
          <apply_if contourDensity="low" additional="contourtype=20f" order="-1"/>
          <apply_if contourDensity="low" additional="contourtype=40f" order="-1"/>
        </case>
      </switch>
    </switch>

  </order>

  <text>

    <!-- TEXT - VILLAGE -->
    <switch minzoom="3" textColor="$placeCityVillageTextColor" textWrapWidth="20" textBold="true" textDy="-4">
