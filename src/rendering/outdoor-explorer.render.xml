        <case minzoom="18" textSize="18"/>
      </apply_if>
    </switch> -->


  </text>

  <point>

    <!-- POINT - VIEWPOINT -->
    <case minzoom="14" tag="tourism" value="viewpoint" icon="topo_viewpoint" shield="topo_viewpoint_shield" iconOrder="85" iconVisibleSize="10" intersectionSizeFactor="2"/>

    <!-- POINT - PEAK -->
    <case minzoom="14" tag="natural" value="peak" iconOrder="65" icon="natural_peak" iconVisibleSize="10" intersectionSizeFactor="2" />

    <!-- POINT - FORD -->
    <case minzoom="14" tag="ford" value="yes" icon="topo_ford" shield="topo_ford_shield" iconVisibleSize="10" intersectionSizeFactor="2"/>

    <!-- POINT - TEMPLE -->
    <case minzoom="14" tag="amenity" value="place_of_worship" iconOrder="40" iconVisibleSize="10" intersectionSizeFactor="2">
      <case additional="religion=christian" icon="topo_christian" shield="topo_christian_shield"/>
      <case additional="religion=jewish" icon="topo_jewish" shield="topo_jewish_shield"/>
      <case additional="religion=muslim" icon="topo_muslim" shield="topo_muslim_shield"/>
      <case additional="religion=sikh" icon="religion_sikh" shield="topo_sikh_shield"/>
      <case additional="religion=buddhist" icon="topo_buddhist" shield="topo_buddhist_shield"/>
      <case additional="religion=hindu" icon="topo_hindu" shield="topo_hindu_shield"/>
      <case additional="religion=shinto" icon="religion_shinto" shield="topo_shinto_shield"/>
      <case additional="religion=taoist" icon="religion_taoist" shield="topo_taoist_shield"/>
      <case icon="topo_religion_unknown" shield="topo_religion_unknown_shield"/>
    </case>

    <!-- POINT - WATERFALL -->
    <case minzoom="14" tag="waterway" value="waterfall" icon="topo_waterfall" shield="topo_waterfall_shield"/>

    <!-- POINT - CAVE -->
    <case minzoom="14" tag="natural" value="cave_entrance" icon="topo_cave" shield="topo_cave_shield" iconOrder="85"/>

    <!-- POINT - NOEXIT -->
    <case minzoom="14" tag="noexit" value="yes" icon="" >
      <apply_if maxzoom="14" icon="barrier_small_black_bold_2"/>
      <apply_if minzoom="15" icon="barrier_small_black_bold_1"/>
    </case>

    <!-- POINT - GATE -->
    <case minzoom="14" tag="barrier" value="log" icon="mm_hazard_yes">
    </case>

    <case minzoom="14" tag="barrier" value="" >
      <apply_if maxzoom="14" icon="barrier_small_red_bold_2"/>
      <apply_if minzoom="15" icon="barrier_small_red_bold_1"/>
    </case>

    <!-- POINT - FIXME -->
    <case hideUnconfirmed="false" minzoom="14" tag="fixme_tag" value="yes" iconOrder="128" icon="fixme_tag_lowzoom_yes"  iconVisibleSize="10" intersectionSizeFactor="2" />

  </point>

  <polygon>
    <!-- POLYGON - LANDUSE -->
    <switch>
      <case minzoom="11" tag="natural" value="beach" color_0="$beachColor"/>
      <case minzoom="11" tag="natural" value="sand" color_0="$sandColor"/>
      <case minzoom="6" tag="natural" value="desert" color="$desertColor"/>
      <switch minzoom="6">
        <case tag="natural" value="wood"/>
        <case tag="landuse" value="forest"/>
        <apply color="$woodColor" color_0="$woodColor"/>
      </switch>
      <case minzoom="12" tag="natural" value="scrub">
        <apply_if maxzoom="13" color="#b3e3b5"/>
        <apply_if minzoom="14" color_0="#B3E3B5" />
      </case>
      <case minzoom="12" tag="landuse" value="orchard" color="$landuseOrchardColor" color_0="$landuseOrchardColor" color_2="#499720">
      </case>
      <switch minzoom="6" color="$grassColor">
        <case tag="natural" value="grassland"/>
        <case tag="landuse" value="meadow"/>
        <case minzoom="12" tag="landuse" value="grassland"/>
      </switch>
      <switch minzoom="11">
        <case tag="landuse" value="grass"/>
        <case tag="natural" value="heath"/>
        <apply color="$grassColor"/>
      </switch>
      <case minzoom="9" tag="landuse" value="residential" color="$landuseResidentialColor" />
      <case minzoom="6" tag="landuse" value="farmland" color="$farmlandColor"/>
      <case minzoom="12" tag="landuse" value="farmyard" color="$farmyardColor"/>
    </switch>

    <!-- POLYGON - WATER -->
    <switch>
      <switch>
        <case minzoom="1" tag="natural" value="coastline"/>
        <switch>
          <case minzoom="4" tag="natural" value="water"/>
          <case minzoom="10" tag="waterway" value="riverbank"/>
          <case minzoom="7" tag="landuse" value="reservoir"/>
          <case minzoom="7" tag="landuse" value="water"/>
          <case minzoom="10" tag="landuse" value="basin"/>
          <apply color_2="$waterColor" strokeWidth_2="0"/>
        </switch>
      </switch>
      <apply color="$waterColor"/>
      <apply_if additional="intermittent=yes" strokeWidth_2="1:0" color_2="#7479ec" pathEffect_2="7_5"/>
    </switch>

    <!-- POLYGON - LAND -->
    <switch>
      <case tag="place" value="island"/>
      <case tag="place" value="islet"/>
      <case tag="natural" value="land"/>
      <apply color="$defaultColor"/>
    </switch>

    <switch>
      <case tag="building" value=""/>
      <apply color="$buildingColor" />
    </switch>

  </polygon>

  <line>
    <!-- LINE - COASTLINE -->
    <case minzoom="1" tag="natural" value="coastline_broken" color="#111111" strokeWidth=":1"/>
    <case minzoom="5" tag="natural" value="coastline_line" color="#4580cf" strokeWidth=":1"/>

    <!-- LINE - HIGHWAY-->
    <switch>
      <switch minzoom="6">
        <case tag="highway" value="motorway" />
        <case tag="highway" value="motorway_link" />
        <case tag="highway" value="trunk" />
        <case tag="highway" value="trunk_link" />
        <case tag="highway" value="primary" />
        <case tag="highway" value="primary_link" />
        <case tag="highway" value="secondary" />
        <case tag="highway" value="secondary_link" />
        <!-- <case tag="highway" value="tertiary"/>
        <case tag="highway" value="tertiary_link"/> -->
        <apply_if maxzoom="6" strokeWidth="0.5"/>
        <apply_if minzoom="7" strokeWidth="0.75"/>
        <apply_if minzoom="8" strokeWidth="1"/>
        <apply_if maxzoom="9" strokeWidth="1.25" />
        <apply_if minzoom="10" strokeWidth="1.5" />
        <apply_if minzoom="11" strokeWidth="1.75" />
        <apply_if minzoom="12" strokeWidth="2" />
        <apply_if minzoom="13" strokeWidth="2.5" />
        <apply_if maxzoom="14" strokeWidth="3.25" />
        <apply_if maxzoom="15" strokeWidth="4"/>
        <apply_if maxzoom="16" strokeWidth="4.75" />
        <apply_if maxzoom="17" strokeWidth="5.5"/>
        <apply_if maxzoom="18" strokeWidth="6.25" />
        <apply_if minzoom="19" strokeWidth="7" />
        <apply
          color="$majorRoadStrokeColor"
          shadowColor="$majorRoadCasingColor"
          cap="ROUND"
          shadowRadius="1"/>
      </switch>

      <!-- MINOR ROARDS-->
      <switch minzoom="11">
        <case tag="highway" value="tertiary"/>
        <case tag="highway" value="tertiary_link"/>
        <case tag="highway" value="unclassified" />
        <case tag="highway" value="living_street" />
        <case tag="highway" value="residential" />
        <case tag="highway" value="track" />
        <case tag="highway" value="service" />
        <case tag="highway" value="busway" />
        <case tag="highway" value="pedestrian" />
        <case tag="highway" value="escape" />
        <case tag="highway" value="raceway" />
        <apply_if maxzoom="13" strokeWidth="1.5" />
        <apply_if minzoom="14" strokeWidth="2.25"  />
        <apply_if minzoom="15" strokeWidth="3" />
        <apply_if minzoom="16" strokeWidth="3.75" />
        <apply_if minzoom="17" strokeWidth="4.5" />
        <apply_if minzoom="18" strokeWidth="5.25" />
        <apply_if minzoom="19" strokeWidth="6" />
        <apply
          color="$minorRoadStrokeColor"
          shadowColor="$minorRoadCasingColor"
          cap="ROUND"
          shadowRadius="1"
        />
      </switch>

      <!-- PATHS -->
      <switch minzoom="13">
        <case tag="highway" value="path" />
        <case tag="highway" value="footway" />
        <case tag="highway" value="cycleway" />
        <case tag="highway" value="bridleway" />
        <case tag="highway" value="steps" />
        <apply_if maxzoom="13" strokeWidth="0.5" />
        <apply_if maxzoom="14" strokeWidth="0.75" />
        <apply_if minzoom="15" strokeWidth="1" />
        <apply_if minzoom="16" strokeWidth="1.25" />
        <apply_if minzoom="17" strokeWidth="1.5" />
        <apply_if minzoom="18" strokeWidth="1.75" />
        <apply_if minzoom="19" strokeWidth="2" />
        <apply
          shadowColor="$pathCasingColor"
          color="$pathStrokeColor"
          cap="ROUND"
          shadowRadius="1"
        />
      </switch>
    </switch>

    <!-- LINE - WATER -->
    <switch>
      <switch minzoom="12">
        <case tag="waterway" value="stream" />
        <apply_if minzoom="13" maxzoom="13" strokeWidth="1.2"/>
        <apply_if minzoom="14" maxzoom="14" strokeWidth="1.5"/>
        <apply_if minzoom="15" maxzoom="16" strokeWidth="1.2:1.2"/>
        <apply_if minzoom="17" maxzoom="17" strokeWidth="1.4:1.4"/>
        <apply_if minzoom="18" strokeWidth="2:2"/>
      </switch>
      <switch minzoom="5" >
        <case tag="waterway" value="river">
          <apply minzoom="13" >
            <case maxzoom="13" />
            <case maxzoom="14" />
            <case minzoom="15" />
            <apply_if additional="intermittent=yes"/>
          </apply>
        </case>
        <case tag="waterway" value="canal">
          <apply minzoom="15">
            <apply_if additional="intermittent=yes"/>
          </apply>
        </case>
        <apply>
          <apply_if maxzoom="9" strokeWidth="0.6"/>
          <apply_if minzoom="10" maxzoom="12" strokeWidth="1:1"/>
          <apply_if minzoom="13" maxzoom="13" strokeWidth="1.2:1.2"/>
          <apply_if minzoom="14" maxzoom="14" strokeWidth="1.5:1.5"/>
          <apply_if minzoom="15" maxzoom="16" strokeWidth="1.8:1.8"/>
          <apply_if minzoom="17" maxzoom="17" strokeWidth="2.2:2.2"/>
          <apply_if minzoom="18" strokeWidth="2.5:2.5"/>
        </apply>
      </switch>

      <switch minzoom="13">
        <case tag="waterway" value="ditch"/>
        <case tag="waterway" value="drain"/>
        <apply>
          <apply_if minzoom="13" maxzoom="13" strokeWidth="1"/>
          <apply_if minzoom="14" maxzoom="14" strokeWidth="1.3"/>
          <apply_if minzoom="15" maxzoom="16" strokeWidth="1:1"/>
          <apply_if minzoom="17" maxzoom="17" strokeWidth="1.2:1.2"/>
          <apply_if minzoom="18" strokeWidth="1.7:1.7"/>
        </apply>
      </switch>
      <apply_if additional="intermittent=yes">
        <case maxzoom="13" pathEffect="4.5_3"/>
        <case maxzoom="14" pathEffect="6_4"/>
        <case maxzoom="16" pathEffect="8_5"/>
        <case maxzoom="17" pathEffect="9_5.5"/>
        <case minzoom="18" pathEffect="12_7"/>
      </apply_if>
      <apply color="$waterColor" onewayArrowsColor="#000000FF" />
    </switch>

    <!-- LINE - CONTOUR -->
    <switch tag="contour" value="elevation">
      <case minzoom="11" contourLines="11"/>
      <case minzoom="12" contourLines="12"/>
      <case minzoom="13" contourLines="13"/>
      <case minzoom="13" contourLines=""/>
      <case minzoom="14" contourLines="14"/>
      <case minzoom="15" contourLines="15"/>
      <case minzoom="16" contourLines="16"/>
      <apply>
        <switch>
          <case minzoom="15" additional="contourtype=5m" color="$contourLineColor"/>
          <case minzoom="13" additional="contourtype=20f" color="$contourLineColor"/>
          <apply>
            <case contourWidth="thin" strokeWidth="0.3"/>
            <case contourWidth="medium" strokeWidth="0.6"/>
            <case contourWidth="thick" strokeWidth="1.1"/>
            <case strokeWidth="0.3"/>
          </apply>
        </switch>
        <switch>
          <case minzoom="13" additional="contourtype=10m" color="$contourLineColor"/>
          <case minzoom="12" additional="contourtype=20m" color="$contourLineColor"/>
          <apply>
            <case contourWidth="thin" strokeWidth="0.5"/>
            <case contourWidth="medium" strokeWidth="1"/>
            <case contourWidth="thick" strokeWidth="1.5"/>
            <case strokeWidth="0.5"/>
          </apply>
        </switch>
        <switch minzoom="11">
          <case additional="contourtype=50m" color="$contourLineColor50m"/>
          <apply>
            <case contourWidth="thin" strokeWidth="0.6"/>
            <case contourWidth="medium" strokeWidth="0.9"/>
            <case contourWidth="thick" strokeWidth="1.3"/>
            <case strokeWidth="0.6"/>
          </apply>
        </switch>
        <switch minzoom="11">
          <case additional="contourtype=100m" color="$contourLineColor100m"/>
          <case additional="contourtype=200f" color="$contourLineColor100m"/>
          <apply>
            <case contourWidth="thin" strokeWidth="1"/>
            <case contourWidth="medium" strokeWidth="1.3"/>
            <case contourWidth="thick" strokeWidth="1.9"/>
            <case strokeWidth="1"/>
          </apply>
        </switch>
        <switch minzoom="11">
          <case additional="contourtype=1000f" color="$contourLineColor100m"/>
          <apply>
            <case contourWidth="thin" strokeWidth="1.3"/>
            <case contourWidth="medium" strokeWidth="1.5"/>
            <case contourWidth="thick" strokeWidth="2.1"/>
            <case strokeWidth="1.3"/>
          </apply>
        </switch>
      </apply>
    </switch>

    <!-- LINE - ADMIN LEVELS -->
    <switch minzoom="2">
      <case tag="admin_level" value="0"/>
      <case tag="admin_level" value="1"/>
      <case tag="admin_level" value="2"/>
      <apply color="$boundaryColorOuter">
        <case maxzoom="2" strokeWidth="0.7"/>
        <case maxzoom="3" strokeWidth="0.9" strokeWidth_0="0.8"/>
        <case maxzoom="4" strokeWidth="1:1" strokeWidth_0="0.8"/>
        <case maxzoom="5" strokeWidth="2:2" strokeWidth_0="1"/>
        <case maxzoom="6" strokeWidth="3:3" strokeWidth_0="1.3"/>
        <case maxzoom="8" strokeWidth="4:4" strokeWidth_0="1:1"/>
        <case minzoom="9" strokeWidth="6:6" strokeWidth_0="1:1"/>
      </apply>
    </switch>
  </line>
</renderingStyle>