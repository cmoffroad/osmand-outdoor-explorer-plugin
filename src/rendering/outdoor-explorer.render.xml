      </switch>
      <switch>
        <case minzoom="13" tag="place" value="hamlet" textSize="12" textOrder="40" nameTag="" intersectionMargin="15">
      <!--     <apply_if minzoom="16" textSize="16"/>
          <apply_if minzoom="17" textSize="17"/> -->
